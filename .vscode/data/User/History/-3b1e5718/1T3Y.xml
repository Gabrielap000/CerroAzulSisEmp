                            </VBox>
                        </items>
                    </VBox>
                </items>
            </VBox>
        </f:content>
        <f:dependents>
            <Dialog showHeader="false" stretch="true" class="sapUiCompSmartFullScreenDialog sapUiSizeCompact">
                <content/>
            </Dialog>
        </f:dependents>
    </f:DynamicPage>
</mvc:View>
