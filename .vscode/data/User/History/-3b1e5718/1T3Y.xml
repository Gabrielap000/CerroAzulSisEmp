                                    <Table width="100%" noDataText="No data" showSeparators="All" growing="true" growingScrollToLoad="true" growingThreshold="20" mode="MultiSelect" class="sapUiCompSmartTable" items="{path:'/KpiSet', templateShareable:true}">
