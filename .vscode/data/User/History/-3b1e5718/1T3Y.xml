                                                </chart:dimensions>
                                                <chart:measures>
                                                    <data:Measure name="Valor" label="Valor KPI" role="axis2"/>
                                                </chart:measures>
                                            </chart:Chart>
                                        </items>
                                    </VBox>
                                </items>
                            </VBox>
                            <VBox visible="{= (${alpModel>/contentView} === 'table' || ${alpModel>/contentView} === 'charttable') }" class="sapSmartTemplatesAnalyticalListPageTableContainer">
