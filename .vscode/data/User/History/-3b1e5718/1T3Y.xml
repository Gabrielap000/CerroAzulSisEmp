<mvc:View xmlns:mvc="sap.ui.core.mvc" controllerName="com.sap.build.standard.logisticaDeAbastecimiento.controller.AnalysisKpi" xmlns:f="sap.f" xmlns="sap.m" xmlns:layout="sap.ui.layout" xmlns:smartvariants="sap.ui.comp.smartvariants" xmlns:filterbar="sap.ui.comp.filterbar" xmlns:chart="sap.chart" xmlns:data="sap.chart.data">
    <f:DynamicPage headerExpanded="true" showFooter="false" class="sapSmartTemplatesAnalyticalListPageDynamicPage sapUiSizeCompact" fitContent="true">
